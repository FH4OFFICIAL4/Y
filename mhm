loadstring(game:HttpGet('https://pastebin.com/raw/6TW7RGz3', true))()
local example = library:AddWindow('ABD Copies')
local w = example:AddTab('Give Stand')
 
local stand = ""
local yes = w:AddDropdown("Stand", function(ddd)
stand = ddd
print(stand)
end)
for i, v in pairs(game.Lighting:GetChildren()) do
 if v:IsA("LocalScript") then
  yes:Add(v.Name)
 end
end
w:AddButton("Give", function()
local mt = getrawmetatable(game)
local oldNameCall = mt.__namecall
setreadonly(mt, false)
mt.__namecall = newcclosure(function(self, ...)
   local Method = getnamecallmethod()
   local Args = {...}
 
   if Method == "FireServer" and self.Name == "CheckAndGiveStand" then
       game.Lighting:WaitForChild(stand):Clone().Parent = game.Players.LocalPlayer.Backpack
       for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
           if v:IsA("Script") then
               v.x1zy:Destroy()
               v.x1zy:Destroy()
           end
       end
return nil
   end
   return oldNameCall(self, unpack(Args))
end)
setreadonly(mt, true)
 
game:GetService("Players").LocalPlayer.Character:BreakJoints()
end)
